@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix language: <http://id.loc.gov/vocabulary/iso639-1/>.
@prefix wd: <https://www.wikidata.org/wiki/>.
@prefix loc: <http://localhost/>.
@prefix mod: <https://w3id.org/mod#>.

loc:new a mod:SemanticArtefact.

{{#keyword}}
loc:new dcat:keyword {{&keyword}}.
{{/keyword}}

{{#contributor}}
loc:new dct:contributor {{&contributor}}.
{{/contributor}}

loc:new mod:acronym "{{acronym}}".

loc:new mod:artefactType mod:{{art_type}}.

loc:new mod:designedForTask ocmv:{{purpose}}.

loc:new ocmv:context ocmv:{{context}}.

loc:new ocmv:domain ocmv:{{domain}}.