@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix loc: <http://localhost/>.
@prefix odrl: <http://www.w3.org/ns/odrl/2/>.

loc:new
    dct:isPartOf <{{&parent_url}}>;
    dct:hasVersion "{{version}}";
    dct:title "{{title}}".

{{#description}}
loc:new dct:description "{{description}}".
{{/description}}
{{#publisher_url}}
loc:new dct:publisher <{{&publisher_url}}>.
{{/publisher_url}}
{{#license_url}}
loc:new dct:license <{{&license_url}}>.
{{/license_url}}
{{#access}}
loc:new {{access_type}} <{{&access}}>.
{{/access}}