@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix language: <http://id.loc.gov/vocabulary/iso639-1/>.
@prefix wd: <https://www.wikidata.org/wiki/>.
@prefix loc: <http://localhost/>.
@prefix mod: <https://w3id.org/mod#>.
@prefix freq: <http://purl.org/cld/terms/Frequency/>.

loc:new a dcat:Dataset.

{{#keyword}}
loc:new dcat:keyword {{&keyword}}.
{{/keyword}}

{{#contributor}}
loc:new dct:contributor {{&contributor}}.
{{/contributor}}

loc:new mod:acronym "{{acronym}}".

{{#frequency}}
loc:new dct:accrualPeriodicity freq:{{frequency}}.
{{/frequency}}

{{#temporalCoverage}}
loc:new dct:temporal "{{temporalCoverage}}".
{{/temporalCoverage}}

{{#temporalResolution}}
loc:new dct:temporalResolution "{{&temporalResolution}}".
{{/temporalResolution}}